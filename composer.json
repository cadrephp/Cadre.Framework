{
    "name": "cadre/framework",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "atlas/orm": "^1.3.0",
        "cadre/cliadr": "^0.2.0",
        "cadre/domain-session": "^0.4.0",
        "cadre/module": "^0.5.2",
        "dflydev/fig-cookies": "^1.0.0",
        "oscarotero/psr7-middlewares": "^3.20.0",
        "psr/cache": "^1.0.1",
        "radar/adr": "^1.1.0",
        "relay/middleware": "^1.2.0",
        "robmorgan/phinx": "^0.8.1",
        "twig/twig": "^1.34.4",
        "zendframework/zend-diactoros": "^1.4.0",
        "cache/void-adapter": "^0.4.1"
    },
    "require-dev": {
        "pds/skeleton": "^1.0.0",
        "phing/phing": "^2.16.0",
        "phpstan/phpstan": "^0.7.0",
        "phpunit/dbunit": "^3.0.0",
        "phpunit/phpunit": "^6.2.3",
        "squizlabs/php_codesniffer": "^3.0.1"
    },
    "autoload": {
        "psr-4": {
            "Cadre\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cadre\\Framework\\": "tests/",
            "PHP_CodeSniffer\\": "vendor/squizlabs/php_codesniffer/src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Shell",
            "email": "andrew@andrewshell.org"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.cadrephp.com"
        }
    ],
    "extra": {
        "cadre": {
            "modules": [
                "Cadre\\Framework\\Module\\AtlasOrm",
                "Cadre\\Framework\\Module\\Core",
                "Cadre\\Framework\\Module\\Session"
            ]
        }
    },
    "minimum-stability": "stable"
}
